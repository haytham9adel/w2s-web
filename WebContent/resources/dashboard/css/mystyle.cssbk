.ver-inline-menu {
  padding: 0;
  margin: 0;
  list-style: none;
}
.ver-inline-menu li {
  position: relative;
  margin-bottom: 1px;
}
.ver-inline-menu li:hover {
  position: relative;
  margin-bottom: 1px;
  background:#000;
}
.ver-inline-menu li a {
  font-size: 14px;
  color: #557386;
  display: block;
  background: #f0f6fa;
  border-left: solid 2px #c4d5df;
  text-decoration: none;
  border: 1px solid #ddd;
  
}
.ver-inline-menu li i {
  width: 37px;
  height: 37px;
  display: inline-block;
  color: #F3BE4E;
  font-size: 15px;
  padding: 12px 10px 10px 8px;
  margin: 0 8px 0 0;
  text-align: center;
  background: rgba(223, 178, 38, 0.16) !important;
}
a, a:focus, a:hover, a:active {
    outline: 0 none;
}
.ver-inline-menu li a:hover {
  background: #ECECEC;
}
.para14{
	font-size:14px;
	font-weight: 500;
	}
.para16{
