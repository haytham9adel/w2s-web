.hidden {
    display:none;
}
#uploadTrigger:hover  i {
    color: rgb(255, 255, 255);
    opacity: 1;
    
}
.profile-thumb{ position:relative; overflow:hidden;}
.profile_caption{ background: rgba(0, 0, 0, 0.2) none repeat scroll 0 0;
    bottom: -60px;
    color: rgb(255, 255, 255);
    display: inline-block;
    left: 0;
    line-height: 26px;
    padding: 10px;
    position: absolute;
    transition: all 0.2s ease-in-out 0s;
    width: 100%;  -webkit-transition: all 0.2s ease-in-out 0s; -moz-transition: all 0.2s ease-in-out 0s;   }
 .profile-thumb:hover .profile_caption{bottom:0;}
 .profile-thumb i.fa {
    color: rgb(68, 68, 68);
    float: left;
    margin-right: 5px;
}
.profile_caption > span {
    float: left;
    color: rgb(211, 211, 211);
    font-size: 10px;
    font-weight: bold;
}

.profile-thumb #uploadTrigger {
       bottom: 10px;
    left: 10px;
    position: absolute;
}
.profile-thumb:hover #uploadTrigger { bottom:-50px;}}


