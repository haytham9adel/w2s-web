@charset "utf-8";
/* CSS Document */

@font-face {
    font-family: 'proxima_nova_rgregular';
    src: url('../fonts/ProximaNova-Regular_0-webfont.eot');
    src: url('../fonts/ProximaNova-Regular_0-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/ProximaNova-Regular_0-webfont.woff2') format('woff2'),
         url('../fonts/ProximaNova-Regular_0-webfont.woff') format('woff'),
         url('../fonts/ProximaNova-Regular_0-webfont.ttf') format('truetype'),
         url('../fonts/ProximaNova-Regular_0-webfont.svg#proxima_nova_rgregular') format('svg');
    font-weight: normal;
    font-style: normal;

}

@font-face {
    font-family: 'proxima_nova_rgbold';
    src: url('../fonts/ProximaNova-Bold_0-webfont.eot');
    src: url('../fonts/ProximaNova-Bold_0-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/ProximaNova-Bold_0-webfont.woff2') format('woff2'),
         url('../fonts/ProximaNova-Bold_0-webfont.woff') format('woff'),
         url('../fonts/ProximaNova-Bold_0-webfont.ttf') format('truetype'),
         url('../fonts/ProximaNova-Bold_0-webfont.svg#proxima_nova_rgbold') format('svg');
    font-weight: normal;
    font-style: normal;

}


@font-face {
    font-family: 'proxima_novalight';
    src: url('../fonts/ProximaNova-Light_0-webfont.eot');
    src: url('../fonts/ProximaNova-Light_0-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/ProximaNova-Light_0-webfont.woff2') format('woff2'),
         url('../fonts/ProximaNova-Light_0-webfont.woff') format('woff'),
         url('../fonts/ProximaNova-Light_0-webfont.ttf') format('truetype'),
         url('../fonts/ProximaNova-Light_0-webfont.svg#proxima_novalight') format('svg');
    font-weight: normal;
    font-style: normal;

}

@font-face {
    font-family: 'proxima_novasemibold';
    src: url('../fonts/ProximaNova-Semibold_0-webfont.eot');
    src: url('../fonts/ProximaNova-Semibold_0-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/ProximaNova-Semibold_0-webfont.woff2') format('woff2'),
         url('../fonts/ProximaNova-Semibold_0-webfont.woff') format('woff'),
         url('../fonts/ProximaNova-Semibold_0-webfont.ttf') format('truetype'),
         url('../fonts/ProximaNova-Semibold_0-webfont.svg#proxima_novasemibold') format('svg');
    font-weight: normal;
    font-style: normal;

}

body{
 padding:0;
 margin:0;
 font-family: 'proxima_nova_rgregular';
 color:#333;
 font-size:13px;
 background:#eeeeee;	
 }

.wrapper{ 
  position:relative;
  width:100%;
}

.head-top{ 
 background:#373737;
 min-height:39px;
 }

.user-panel .btn-default {
    background: none repeat scroll 0 0 #eeeeee;
    border-radius: 5px;
    font-size: 13.33px;
    margin: 8px 5px 0;
    padding: 1px 13px;
    vertical-align: middle;
}

.logo {
    height: 100%;
    vertical-align: middle;

} 

.head-bottom {
    background: none repeat scroll 0 0 #fff;
	padding-top: 5px;
}

.head-bottom .main-nav {
    margin-top:35px;
}
.main-nav {
    float: right !important;
}

.main-nav .navbar-nav li a {
    color: #333;
    font-size: 17.33px;
    padding: 18px 19px 29px;
    text-align: center;
    text-decoration:none;
}

.main-nav .navbar-nav li > a:focus, .nav > li > a:hover {
    background-color:#eeeeee;
    text-decoration: none;
}

.head-bottom .main-nav .nav .active a {
    background:#eeeeee;
    color: #333;
}


.three-block-wrap {
    background: none repeat scroll 0 0 #f2f2f2;
    border: 1px solid #ddd;
    text-align: center;
}

.white-bg {
    background: none repeat scroll 0 0 #fff;
    padding: 33px;
}

.three-block-wrap p {
    padding: 15px 37px;
	font-family: 'proxima_novalight';
	color:#999;
	
}

.three-block-wrap a {
    background: none repeat scroll 0 0 #333;
    border: medium none;
    border-radius: 0;
    color: #fff;
    font-size: 15.33px;
    margin-bottom: 15px;
    padding: 10px 20px;
}

.three-block-wrap a:hover{ 
  background: none repeat scroll 0 0 #333;
  color: #fff;
}

.who-we-are-block .fa-users {
    color: #c5c5c5;
    font-size: 201px;
    margin: 33px;
    text-align: center;
}

.who-we-are-block h1{
 font-size:43.33px;
 text-transform:uppercase;
 margin-bottom: 0;
 margin-top: 45px;
 }
 
 .who-we-are-block span{
  font-size:25.33px;
  font-family: 'proxima_novalight';	
  text-transform:uppercase; 
}

.who-we-are-block p{
  color: #c5c5c5;
  font-family: 'proxima_novalight';
  font-size:17.33px;
}

.who-we-are-block a {
    background: none repeat scroll 0 0 rgba(0, 0, 0, 0);
    border: medium none;
    font-size: 13.33px;
    padding: 0;
    text-transform: uppercase;
}

.tracking-system-blcok{ 
  background-image:url(../Images/tackin-system-bg.jpg);
  background-repeat:no-repeat;
  background-position:center;
  background-size:100%;
  padding-bottom: 30px;
}

.tracking-system-blcok h2 {
    color: #fff;
    margin: 20px 0;
    text-align: center;
	font-family: 'proxima_nova_rgbold';
	font-size:29.33;
	text-transform:uppercase;
}

.tracking-system-blcok .item-cover {
    background: none repeat scroll 0 0 #fff;
    border: 1px solid #ccc;
    display: block;
    margin: 15px 18px;
    padding: 10px;
    text-align: center;
    width: 290px;
}

.item-cover h4 {
    text-align: left;
}

.item-cover p{ 
  text-align:left;
  font-family: 'proxima_novalight';
  }

.counting-visual{position:relative;}
.counting-visual > span {
    background: none repeat scroll 0 0 #ff4b64;
    border-radius: 58%;
    color: #fff;
    display: block;
    font-size: 36px;
    font-weight: 600;
    height:75px;
    line-height:75px;
    margin:10px auto;
    text-align: center;
    width:75px;
	
} 

.visual-content {
    background: none repeat scroll 0 0 #eee;
    padding:0 15px;
	position:relative;
}

.service-icon {
    float: left;
    padding-top: 31px;
    vertical-align: middle;
    width:60px;
}

.desc {
    float: right;
    vertical-align: top;
    width:190px;
} 

.visual-content::after {
    border-bottom: 34px solid #eee;
    border-left: 20px solid transparent;
    border-right: 20px solid transparent;
    content: "";
    display: inline-block;
    position: relative;
    right: -113px;
    top: -115px;
}

footer{ 
  background:#373737; 
  padding:30px 0;
  margin-top: 15px;
  }
  
.sep-verti {
    border-right: 1px dotted #fff;
    padding-right: 0;
    width: 280px;
}

.headline > h4 {
    color: #fff;
	font-family:'proxima_novasemibold';
}

footer .list-unstyled > li a{
    color: #fff;
    display: block;
    padding: 5px 0;
    text-decoration: none;
	font-size:14px;
}

footer .list-inline > li a{
	
  color: #fff;
    display: block;
    padding: 5px 0;
    text-decoration: none;
	font-size:14px;
}

.btn-u {
    background: none repeat scroll 0 0 #f3be4e;
    border: medium none;
    border-radius: 0 4px 4px 0;
    color: #fff;
    font-size: 23px;
    padding: 0.2px 16px;
    text-align: center;
	height: 34px;
}

.copyright {
    background: none repeat scroll 0 0 #333;
    bottom: -34px;
    color: #999999;
    padding: 10px 0;
    position: relative;
	font-size:14px;
	font-family: 'proxima_novalight';
}
